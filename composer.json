{
    "name": "michaelze/cakephp-annauthorize",
    "description": "AnnAuthorize plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "keywords": [
        "cakephp",
        "authorization",
        "annotations"
    ],
    "homepage": "https://github.com/michaelze/cakephp-annauthorize",
    "require": {
        "php": ">=7.",
        "cakephp/cakephp": "~3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "6"
    },
    "autoload": {
        "psr-4": {
            "AnnAuthorize\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnnAuthorize\\Test\\": "tests",
            "TestApp\\": "tests/test_app",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests"
        }
    }
}
